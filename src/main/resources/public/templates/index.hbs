{{#partial "styles"}}
    <link rel="stylesheet" href="/css/style.css">
{{/partial}}
<h1>SCHOOL-MANAGEMENT-SYSTEM</h1>
{{#if student}}
    {{#each student}}
        <div class="card" style="width: 25rem;">
            <div class="card-body">
                <h5 class="card-title">STUDENT {{name}}</h5>
                <p>student: {{studentName}}</p>
                <p>cont: {{studentContact}}</p>
                <p>room: {{studntRoom}}</p>
            </div>